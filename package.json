{
  "name": "vue-flatpickr-component",
  "version": "7.0.5",
  "description": "Vue.js component for Flatpickr date-time picker",
  "main": "dist/vue-flatpickr.js",
  "browser": "dist/vue-flatpickr.min.js",
  "files": [
    "src",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ankurk91/vue-flatpickr-component.git"
  },
  "keywords": [
    "vue",
    "flatpickr",
    "vue-flatpickr",
    "datepicker",
    "timepicker"
  ],
  "author": "ankurk91",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ankurk91/vue-flatpickr-component/issues"
  },
  "homepage": "https://github.com/ankurk91/vue-flatpickr-component",
  "scripts": {
    "test": "jest",
    "test:watch": "npm run test -- --watch --onlyChanged --notify",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-serve --config=webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --progress --mode production",
    "prepublishOnly": "npm run test && npm run build"
  },
  "dependencies": {
    "flatpickr": "^4.5.1"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.23",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "bootstrap": "^4.1.3",
    "clean-webpack-plugin": "^0.1.18",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.9",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.2",
    "jquery": "^3.3.1",
    "popper.js": "^1.12.9",
    "style-loader": "^0.21.0",
    "unminified-webpack-plugin": "^2.0.0",
    "vee-validate": "^2.1.0-beta.7",
    "vue": "^2.5.17",
    "vue-jest": "^2.6.0",
    "vue-loader": "^15.2.6",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.16.3",
    "webpack-command": "^0.4.1",
    "webpack-serve": "^2.0.2"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  },
  "engines": {
    "node": ">= 4.2.0",
    "npm": ">= 3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "collectCoverage": true,
    "testURL": "http://localhost"
  }
}
