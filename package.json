    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.6.0",
    "style-loader": "^3.3.1",
    "vue": "^3.2.33",
    "vue-loader": "^17.0.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "engines": {
    "node": ">=10.13.0"
  }
